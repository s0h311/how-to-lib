{
  "name": "fantastic-calculator",
  "version": "0.0.1",
  "description": "Just a TypeScript library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build-esm": "esbuild --bundle ./src/index.ts --platform=browser --outdir=dist --packages=external --format=esm --out-extension:.js=.mjs --minify",
    "build-cjs": "esbuild --bundle ./src/index.ts --platform=browser --outdir=dist --packages=external --format=cjs --out-extension:.js=.cjs --minify",
    "test": "vitest",
    "lint": "tsc"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "esbuild": "^0.21.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.4"
  }
}
